{
	"name": "fibers",
	"version": "5.0.0",
	"description": "Cooperative multi-tasking for Javascript",
	"keywords": [
		"fiber",
		"fibers",
		"coroutine",
		"thread",
		"async",
		"parallel",
		"worker",
		"future",
		"promise"
	],
	"homepage": "https://github.com/laverdet/node-fibers",
	"author": "Marcel Laverdet <marcel@laverdet.com> (https://github.com/laverdet/)",
	"main": "fibers",
	"scripts": {
		"install": "node-pre-gyp install --fallback-to-build",
		"test": "node test.js"
	},
	"binary": {
		"module_name": "fibers",
		"module_path": "./lib/v{version}-{node_abi}-{platform}-{libc}-{arch}",
		"package_name": "{node_abi}-{platform}-{libc}-{arch}.tar.gz",
		"host": "https://fibers.s3-us-west-2.amazonaws.com",
		"remote_path": "./v{version}/"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/laverdet/node-fibers.git"
	},
	"license": "MIT",
	"engines": {
		"node": ">=10.0.0"
	},
	"dependencies": {
		"node-gyp": "^7.1.2",
		"node-pre-gyp": "^0.15.0"
	},
	"devDependencies": {
		"aws-sdk": "^2.781.0"
	}
}
